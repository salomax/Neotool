meta {
  name: Sign In with OAuth
  type: graphql
  seq: 4
}

post {
  url: http://{{router}}/graphql
  body: graphql
  auth: inherit
}

body:graphql {
  mutation SignInWithOAuth($input: SignInWithOAuthInput!) {
    signInWithOAuth(input: $input) {
      token
      refreshToken
      user {
        id
        email
      }
    }
  }
}

body:graphql:vars {
  {
    "input": {
      "provider": "google",
      "idToken": "SAMPLE_ID_TOKEN"
    }
  }
}
