meta {
  name: Assign Group To User
  type: graphql
  seq: 13
}

post {
  url: http://{{router}}/graphql
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{jwt_token}}
}

body:graphql {
  mutation AssignGroupToUser($userId: ID!, $groupId: ID!) {
    assignGroupToUser(userId: $userId, groupId: $groupId) {
      id
      groups {
        id
        name
      }
    }
  }
}

body:graphql:vars {
  {
    "userId": "{{userId}}",
    "groupId": "{{groupId}}"
  }
}
