meta {
  name: Request Password Reset
  type: graphql
  seq: 6
}

post {
  url: http://{{router}}/graphql
  body: graphql
  auth: inherit
}

body:graphql {
  mutation RequestPasswordReset($input: RequestPasswordResetInput!) {
    requestPasswordReset(input: $input) {
      success
      message
    }
  }
}

body:graphql:vars {
  {
    "input": {
      "email": "user@example.com"
    }
  }
}
