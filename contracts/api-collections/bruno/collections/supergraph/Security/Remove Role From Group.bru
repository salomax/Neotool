meta {
  name: Remove Role From Group
  type: graphql
  seq: 21
}

post {
  url: http://{{router}}/graphql
  body: graphql
  auth: bearer
}

auth:bearer {
  token: {{jwt_token}}
}

body:graphql {
  mutation RemoveRoleFromGroup($groupId: ID!, $roleId: ID!) {
    removeRoleFromGroup(groupId: $groupId, roleId: $roleId) {
      id
      roles {
        id
        name
      }
    }
  }
}

body:graphql:vars {
  {
    "groupId": "{{groupId}}",
    "roleId": "{{roleId}}"
  }
}
