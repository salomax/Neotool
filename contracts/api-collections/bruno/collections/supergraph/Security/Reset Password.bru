meta {
  name: Reset Password
  type: graphql
  seq: 7
}

post {
  url: http://{{router}}/graphql
  body: graphql
  auth: inherit
}

body:graphql {
  mutation ResetPassword($input: ResetPasswordInput!) {
    resetPassword(input: $input) {
      success
      message
    }
  }
}

body:graphql:vars {
  {
    "input": {
      "token": "RESET_TOKEN",
      "newPassword": "newPassword123"
    }
  }
}
