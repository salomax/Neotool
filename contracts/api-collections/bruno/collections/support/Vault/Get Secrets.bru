meta {
  name: Get Secrets
  type: http
  seq: 1
}

get {
  url: http://{{vault_host}}/v1/secret/data/{{secret_path}}
  body: none
  auth: apikey
}

auth:apikey {
  key: X-Vault-Token
  value: {{vault_token}}
  placement: header
}

vars:pre-request {
  secret_path: jwt/keys/kid-1
}

settings {
  encodeUrl: false
  timeout: 0
}
